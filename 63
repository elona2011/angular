gulp.task('svn.update', function(cb) {
    var cwd = process.cwd();
    var cmd_command = 'TortoiseProc.exe /command:update /path:' + cwd + ' /closeonend:1';
    exec(cmd_command, function(err, stdout, stderr) {
        console.log(stdout);
        console.log(stderr);
        cb(err);
    });
});
